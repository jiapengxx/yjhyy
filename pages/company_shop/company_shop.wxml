<view style='position:relative;width:100%;'>
  <view class="swiper-tab">
    <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">综合</view>
    <view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}" data-current="1" bindtap="swichNav">附近</view>
    <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}" data-current="2" bindtap="swichNav">销量</view>
    <view class="swiper-tab-list {{currentTab==3 ? 'on' : ''}}" data-current="3" bindtap="swichNav">评价</view>
  </view>
  <swiper current="{{currentTab}}" class="swiper-box" duration="300" style='height:{{height}}rpx;' bindchange="bindChange">
    <swiper-item>
      <block wx:for="{{hot_good}}" wx:key="id" wx:for-item="item" wx:for-index="idx">
        <view style='height:582rpx;width:702rpx;margin-left:24rpx;'>
          <view id='{{item.store_id}}' style='position:relative;top:40rpx;height:150rpx;width:500rpx;display:flex;flex-direction:row;'>
            <view id='{{item.store_id}}' bindtap='toBtoC' style='height:150rpx;width:150rpx;'>
              <image src='{{host}}{{item.img}}' style='height:150rpx;width:150rpx;'></image>
            </view>
            <view id='{{item.store_id}}' bindtap='toBtoC' style='position:relative;left:30rpx;width:320rpx;height:150rpx;'>
              <text class='t1'>{{item.name}}\n</text>
              <text class='t2'>{{item.introduce}}\n</text>
              <text class='t4'>{{item.type_name}}</text>
            </view>
            <view style='position:relative;top:110rpx;left:60rpx;width:80rpx;height:50rpx;'>
              <text class='t5'>销量:{{item.num}}</text>
            </view>


            <view style='position:relative;top:0rpx;left:110rpx;width:200rpx;'>
              <image bindtap='callPhone' src='../../images/callPhone.png' style='position:relative;top:-28rpx;left:-130rpx;height:37rpx;width:37rpx;z-index:100;'></image>
              <image id='{{item.store_id}}' bindtap='getPosition' src='../../images/position.png' style='position:relative;top:-28rpx;left:-120rpx;space:emsp;height:37rpx;width:29rpx;z-index:100;'></image>
              <view class='t3'>{{distance1[idx].distance}}km</view>
            </view>
          </view>


          <view wx:for="{{item.goods}}" wx:for-item="ite" wx:for-index="index" style='position:relative;top:80rpx;height:292rpx;width:702rpx;display:flex;flex-direction:row;justify-content:space-between;flex-wrap:wrap;'>
            <view id='{{item.goods[index].product_id}}' style='display:flex;flex-direction:column;justify-content:space-between;flex-wrap:wrap;width:212rpx;height:290rpx;text-align:center;line-height:25rpx;' bindtap='toGoodsDetail'>
              <image style='width:212rpx;height:180rpx;' src='{{host}}/Data/{{item.goods[index].pro_photo_x}}'></image>
              <text style='width:212rpx;height:48rpx;font-size:26rpx;color:#333333;'>{{item.goods[index].pro_name}}</text>
              <text style='width:212rpx;height:30rpx;font-size:24rpx;color:#e91d1d'>￥{{item.goods[index].price}}</text>
            </view>
          </view>
        </view>
        <view style='border-bottom: 0.1rpx solid #777;'></view>
      </block>

    </swiper-item>
