
<view style='height:40rpx;background-color:#ebebeb;'></view>
<view style='margin-left:30rpx;width:690rpx;height:90rpx;border-bottom:1rpx solid #ebebeb;'>

<input bindblur='tel' type='number' style='position:relative;top:24rpx;left:26rpx;font-size:30rpx;color:#333333;width:300rpx;' placeholder='请输入手机号' placeholder-style='font-size:22rpx;color:#999999;'></input>

